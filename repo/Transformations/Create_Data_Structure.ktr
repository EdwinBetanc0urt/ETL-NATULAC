    <sql>--Product Entity Definition
CREATE TABLE IF NOT EXISTS public.products(
productId VARCHAR(20),
name VARCHAR(40),
productClass VARCHAR(20),
unitOfMeasure VARCHAR(20),
barCode VARCHAR(13),
updateTime TIMESTAMP DEFAULT NOW(),
tenantId VARCHAR(100),
fileName VARCHAR(500)
);

--Files Entity Definition
CREATE TABLE IF NOT EXISTS public.files(
tenantId VARCHAR(100),
fileName VARCHAR(500),
recordReadFromFile NUMERIC DEFAULT 0,
recordAffected NUMERIC DEFAULT 0,
processed boolean default false
);

--Customer Entity Definition
CREATE TABLE IF NOT EXISTS public.customers(
taxId VARCHAR(10),
name VARCHAR(40),
country VARCHAR(20),
state VARCHAR(20),
municipality VARCHAR(20),
city VARCHAR(20),
address VARCHAR(50),
latitude NUMERIC DEFAULT 0,
longitude NUMERIC DEFAULT 0,
isFiscalAddress VARCHAR(2),
salesChannel VARCHAR(20),
route VARCHAR(20),
updateTime TIMESTAMP DEFAULT NOW(),
tenantId VARCHAR(100),
fileName VARCHAR(500)
);


--Sales Invoice Definition
CREATE TABLE IF NOT EXISTS public.sales_invoice(
route VARCHAR(20),
taxId VARCHAR(10),
businessPartnerName VARCHAR(40),
country VARCHAR(20),
state VARCHAR(20),
municipality VARCHAR(20),
city VARCHAR(20),
shipmentAddress VARCHAR(50),
productId VARCHAR(20),
barCode VARCHAR(13),
unitOfMeasure VARCHAR(20),
documentDate TIMESTAMP,
documentNumber VARCHAR(10),
productName VARCHAR(40),
vendorName VARCHAR(20),
salesChannel VARCHAR(20),
quantity NUMERIC,
price NUMERIC,
isoCurrency VARCHAR(3),
netAmount NUMERIC,
updateTime TIMESTAMP DEFAULT NOW(),
tenantId VARCHAR(100),
fileName VARCHAR(500)
);

--Sales Invoice Definition
CREATE TABLE IF NOT EXISTS public.inventory(
productId VARCHAR(20),
productName VARCHAR(40),
barCode VARCHAR(13),
availability NUMERIC,
updateTime TIMESTAMP DEFAULT NOW(),
inventoryDate TIMESTAMP,
tenantId VARCHAR(100),
fileName VARCHAR(500)
);

CREATE TABLE IF NOT EXISTS public.tenant_access
(
tenantid VARCHAR(100),
email VARCHAR(255)
);

CREATE INDEX IF NOT EXISTS IX_Files ON files(tenantId, fileName); 

CREATE OR REPLACE FUNCTION affected_records_files() 
RETURNS TRIGGER 
LANGUAGE 'plpgsql' 
COST 100
VOLATILE NOT LEAKPROOF 
AS $BODY$
    BEGIN
        IF (TG_OP = 'UPDATE') THEN
            UPDATE files SET recordAffected = recordAffected +1 WHERE tenantId = NEW.tenantId AND fileName = NEW.fileName;
		END IF;
        IF (TG_OP = 'INSERT') THEN
            UPDATE files SET recordAffected = recordAffected +1 WHERE tenantId = NEW.tenantId AND fileName = NEW.fileName;
        END IF;
        RETURN NULL;
    END
$BODY$;


CREATE TRIGGER  products_file_update
AFTER INSERT OR UPDATE ON products
    FOR EACH ROW EXECUTE FUNCTION affected_records_files();
	
CREATE TRIGGER customers_file_update
AFTER INSERT OR UPDATE ON customers
    FOR EACH ROW EXECUTE FUNCTION affected_records_files();

CREATE TRIGGER sales_invoice_file_update
AFTER INSERT OR UPDATE ON sales_invoice
    FOR EACH ROW EXECUTE FUNCTION affected_records_files();
	

CREATE TRIGGER inventory_file_update
AFTER INSERT OR UPDATE ON inventory
    FOR EACH ROW EXECUTE FUNCTION affected_records_files();


CREATE INDEX IF NOT EXISTS I_Key_Products ON Products(tenantId, productId);
CREATE INDEX IF NOT EXISTS I_Key_Customers ON Customers(tenantId, taxId);
CREATE INDEX IF NOT EXISTS I_Key_Inventory ON Inventory(tenantId, productId);
CREATE INDEX IF NOT EXISTS I_Key_Sales_Invoice ON Sales_Invoice(taxId, tenantId, documentNumber, productId);
ALTER TABLE IF EXISTS public.sales_invoice ADD COLUMN IF NOT EXISTS updateTimeData TIMESTAMP;
ALTER TABLE IF EXISTS public.inventory ADD COLUMN IF NOT EXISTS updateTimeData TIMESTAMP;

