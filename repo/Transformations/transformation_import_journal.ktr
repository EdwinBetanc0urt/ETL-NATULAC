    <sql>SELECT
  NUMCMP AS DocumentNo,
  FECCMP AS DateAcct,
  DESTRA AS Description,
  (CASE WHEN DEBCRE = '1' 
    THEN MONCTB
    ELSE 0
  END) AS AmtSourceDR,
  (CASE WHEN DEBCRE = '2'
    THEN ABS(MONCTB)
    ELSE 0
  END) AS AmtSourceCR,
  COALESCE(TRIM(SUBSTR(CTACTB, 1, 8)), '') AS AccountValue,
  COALESCE(TRIM(SUBSTR(CTACTB, 9, 4)), '') AS UserElement1Value,
  NULL AS UserElement2Value,
  COALESCE(TRIM(SUBSTR(CTACTB, 13, 10)), '') AS BPartnerValue

FROM "public".NMM027
</sql>
